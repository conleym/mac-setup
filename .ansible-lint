# ansible-lint configuration.
# Note that `ansible-lint` must be installed manually via `dev-init.sh`.
#
# Docs at https://ansible.readthedocs.io/projects/lint/
# Rules: https://ansible.readthedocs.io/projects/lint/rules/
---
exclude_paths:
  - .idea/
  - .cache/

enable_list:
  - no-log-password

skip_list:
  - fqcn[action]
  - fqcn[action-core]
  - git-latest
  - meta-no-info
  - unnamed-task
